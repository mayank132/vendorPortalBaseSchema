// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Company {
  id       String    @id @default(cuid())
  name     String    @default("")
  email    String    @unique @default("")
  products Product[] @relation("Company_products")
  vendors  Vendor[]  @relation("Company_vendors")
}

model Product {
  id                    String     @id @default(cuid())
  name                  String     @default("")
  price                 String     @default("")
  category              Category[] @relation("Product_category")
  from_Company_products Company[]  @relation("Company_products")
}

model Category {
  id                    String    @id @default(cuid())
  name                  String    @default("")
  from_Product_category Product[] @relation("Product_category")
}

model Vendor {
  id                   String    @id @default(cuid())
  name                 String    @default("")
  email                String    @unique @default("")
  phone                String    @unique @default("")
  from_Company_vendors Company[] @relation("Company_vendors")
}
